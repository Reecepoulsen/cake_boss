        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading || newValue === '') {
		return;
	}
	
	var ingCost = parseFloat(g_form.getValue("ingredient_cost"));
	var prepCost = parseFloat(newValue);
	var suggestedPrice = ingCost + prepCost;

	g_form.setValue("suggested_price", suggestedPrice.toFixed(2));
}]]></script>
